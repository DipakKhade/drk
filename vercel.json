{
    "version": 2,
    "builds": [
      { "src": ".", "use": "@vercel/node" }
    ],
    "routes": [
      { "handle": "vercel", "src": "/.*" }
    ],
    "functions": { "api/**/*.{js,ts}": { "runtime": "nodejs14.x" } },
    "build": {
      "commands": ["prisma generate", "npm run build"]
    }
  }
  